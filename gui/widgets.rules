MainSprite
{
	Texture
	{
		File = "widgets.png"
		MipLevels = 2
		SampleMode = Linear
		FixTransparentColors = true
	}

	Shader = "gui.shader"
	_highlightTime = -1000
	_unhighlightTime = -1000
	_clickTime = -1000
}
HighlightSprite : MainSprite
{
	Shader = "gui_highlight.shader"
}
SelectedSprite : MainSprite
{
	Shader = "gui_selected.shader"
}

PulsingTextShader = "pulsing.shader"

ArrowSprite
{
	Texture { File="widgets_arrows.png"; MipLevels=2; SampleMode=Linear; }
}

DefaultFont
{
	Files
	[
		"play_msyh.ttf"
		"play_msyh_bold.ttf"
	]

	ForceSameWidths // Force all digits to be the same width.
	[
		"123456789"
		"000000000"
	]
}

ShadowFont : DefaultFont
{
	Passes
	[
		// Black shadow.
		{
			Effects
			[
				{ Type=Blur; BlurAmount=0.25; }
				{ Type=Color; Color=[0, 0, 0, 255*2]; }
			]
		}

		// Regular foreground.
		{
			// Nothing weird.
		}
	]
}

GlowFont : DefaultFont
{
	Passes
	[
		// Glow.
		{
			Effects
			[
				{ Type=Blur; BlurAmount=0.5; }
			]
		}

		// Regular foreground.
		{
			// Nothing weird.
		}
	]
}

ShadowGlowFont : DefaultFont
{
	Passes
	[
		// Black shadow.
		{
			Effects
			[
				{ Type=Blur; BlurAmount=0.25; }
				{ Type=Color; Color=[0, 0, 0, 255*1]; }
			]
		}

		// Glow.
		{
			Effects
			[
				{ Type=Blur; BlurAmount=0.5; }
			]
		}

		// Regular foreground.
		{
			// Nothing weird.
		}
	]
}

MainText
