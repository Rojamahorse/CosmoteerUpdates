Tips
{
	
	Accessibility
	{
		
		Tip = "Wenn du Schwierigkeiten hast, zwischen den Symbolen für befreundete und feindliche Schiffe zu unterscheiden, kannst du ihre Farben unter <good>Einstellungen -> Barrierefreiheit</good> verändern. Außerdem gibt es noch weitere Einstellungen, um Cosmoteer für sehbehinderte Spieler leichter zugänglich zu machen."
		Title = "Einstellungen für Farbenblindheit & Barrierefreiheit"
	}
	AdvancedCareerMode
	{
		
		Tip = "Im Karrieremodus gibt es eine Handvoll erweiterter Optionen, die du ändern kannst, um das Spiel nach deinen Vorlieben anzupassen, etwa den sofortigen Bau von Schiffen oder die Enthüllung des gesamten Sternensystems. Du kannst diese Optionen aktivieren, indem du beim Start eines neuen Spiels im Karrieremodus die <good>Erweiterten Spieloptionen</good> anklickst, oder während eines Spiels <good>Einstellungen -> Aktuelles Spiel -> Erweiterte Spieloptionen</good> aufrufst."
		Title = "Erweiterte Karrieremodus-Optionen"
	}
	AdvancedCommands
	{
		
		Tip = "Wenn du die Schaltflächen für erweiterte Schiffsbefehle nicht sehen kannst, kannst du sie unter <good>Einstellungen -> Spiel -> Erweiterte Schiffsbefehle anzeigen</good> aktivieren."
		Title = "Erweiterte Schiffsbefehle"
	}
	AreYouSure
	{
		
		Tip = "Zahlreiche der Bestätigungsaufforderungen im Stile von <i>„Bist du sicher, dass du ...“</i> können durch Gedrückthalten von <btn id='General.SuppressConfirmation'/> übersprungen werden."
		Title = "Bist du sicher?"
	}
	AttackDefaults
	{
		
		Tip = "Bist du es leid, jedes Mal, wenn du einen Feind angreifst, die Entfernung und die Drehung deines Schiffes einzustellen? Öffne das <img name='hamburger_up'/>-Menü neben dem Namen deines Schiffes und klicke auf <good>Standardangriffswerte speichern</good>. Deine aktuelle Entfernung, deine Drehung und dein Angriffswinkel werden gespeichert, bis du das nächste Mal etwas angreifst."
		Title = "Standardangriffswerte einstellen"
	}
	AutoSave
	{
		
		Tip = "Cosmoteer speichert dein Spiel standardmäßig alle 10 Minuten und bewahrt die letzten 10 automatischen Speicherstände auf. Ältere automatische Speicherungen werden automatisch gelöscht. Die Dauer und die Anzahl der aufbewahrten automatischen Speicherstände kannst du unter <good>Einstellungen -> Autom. Speichern</good> verändern."
		Title = "Automatische Speicherungen"
	}
	BattleHelper
	{
		
		Tip = "Im Kreativmodus kannst du den Kampfhelfer einsetzen, um Kämpfe zwischen zwei verfeindeten Schiffsflotten zu organisieren und durchzuführen. Du kannst den Kampfhelfer über das <img name='hamburger_down'/>-Menü aufrufen."
		Title = Kampfhelfer
	}
	BlueprintMode
	{
		
		Tip = "Mithilfe des „Bauplanmodus“ kannst du vorübergehende, illegale oder unbezahlbare Änderungen am Design deines Schiffes vornehmen, bevor du diese Änderungen tatsächlich festlegst. Klicke auf die <white>Bauplanmodus</white>-Schaltfläche unten links im Schiffseditor, um den Bauplanmodus zu aktivieren."
		Title = Bauplanmodus
	}
	ChangingDifficulty
	{
		
		Tip = "Du kannst den Schwierigkeitsgrad im Karrieremodus jederzeit während des Spielens ändern – und nicht nur beim Start eines neuen Spiels. Um den Schwierigkeitsgrad zu ändern, gehe zu <good>Einstellungen -> Aktuelles Spiel</good>."
		Title = "Wechselnde Schwierigkeit"
	}
	Chatting
	{
		
		Tip = "Im Multiplayer-Modus kannst du durch Drücken von <btn id='Game.Chat'/> den anderen Spielern Textchat-Nachrichten schicken."
		Title = Multiplayer-Chat
	}
	CollisionAvoidance
	{
		
		Tip = "Schiffe werden in der Regel versuchen, um große Hindernisse herum zu navigieren und eine Kollision mit ihnen zu vermeiden. Wenn dir das nicht zusagt, kannst du <btn id='Game.InvertCollisionAvoidance'/> gedrückt halten, während du einen Bewegungsbefehl gibst, um die Kollisionsvermeidung zu deaktivieren, oder sie unter <good>Einstellungen -> Spiel -> Schiffsbefehle -> Kollisionsvermeidung aktivieren</good> dauerhaft ausschalten."
		Title = Kollisionsvermeidung
	}
	ControlGroups
	{
		
		Tip1 = "Du kannst eins oder mehrere deiner Schiffe zu einer „Steuerungsgruppe“ hinzufügen, indem du sie auswählst und dann <white>Strg + F1 bis F8</white> drückst. Anschließend kannst du alle Schiffe in dieser Gruppe durch Drücken von <white>F1 bis F8</white> auswählen."
		Tip2 = "Entsprechend kannst du eine oder mehrere deiner einzelnen Waffen oder anderen Schiffsteile zu Steuerungsgruppen hinzufügen, indem du sie auswählst und dann <white>Strg + 0 bis 9</white> drückst. Anschließend kannst du alle Teile in dieser Gruppe durch Drücken von <white>0 bis 9</white> auswählen."
		Title = Steuerungsgruppen
	}
	CrewCustomization
	{
		
		Tip = "Die Namen und das Aussehen deiner gesamten Crew können angepasst werden! Öffne die Crew-Aufstellung und <white>klicke doppelt</white> auf das Crewmitglied, das du anpassen möchtest."
		Title = Crew-Anpassung
	}
	CrewFavorite
	{
		
		Tip = "Liegen dir einige deiner Crewmitglieder mehr am Herzen als andere? <white>Klicke sie mit der rechten Maustaste</white> in der Crew-Aufstellung an und markiere sie als „Favoriten“. Auf diese Weise weißt du immer, wo sie sind!"
		Title = Crew-Favoriten
	}
	CrewResourceTransfer
	{
		
		Tip = "Du kannst Crew und Ressourcen zwischen deinen Schiffen transferieren. Klicke auf das <img name='hamburger_up'/>-Menü neben dem Namen deines Schiffes (oder in den Bildschirmen für Crew- oder Ressourcenmanagement) und wähle dann <white>Crewmitglieder/Ressourcen transferieren</white>."
		Title = "Crew- & Ressourcentransfer"
	}
	CtrlSelectsParts
	{
		
		Tip = "Wenn du einzelne Waffen und andere Schiffsteile nicht einfach durch Anklicken auswählen kannst, musst du entweder <btn id='Game.SelectParts'/> gedrückt halten oder die Option <good>Einstellungen -> Spiel -> Strg erforderlich zum Auswählen von Teilen</good> deaktivieren."
		Title = "Strg erforderlich zum Auswählen von Teilen"
	}
	CtrlTargeting
	{
		
		Tip1 = "Du kannst <btn id='Game.SelectParts'/> gedrückt halten, um deine Waffen auf gegnerische Schiffe zu richten, ohne sie zuerst angreifen zu müssen."
		Tip2 = "Du kannst auch <btn id='Game.SelectParts'/> gedrückt halten, um deine Waffen bewusst auf befreundete Schiffe zu richten."
		Title = Strg-Zielen
	}
	DecalLayers
	{
		
		Tip = "Klicke das „Auge“-Symbol an, um Aufkleber-Schichten ein- oder auszublenden. Ziehe die Aufkleber-Schichten, um ihre Reihenfolge zu ändern."
		Title = Aufkleber-Schichten
	}
	DeleteDoors
	{
		
		Tip = "Möchtest du mehrere Türen auf einmal löschen, wähle mit dem Auswahlwerkzeug alle Teile aus, deren Türen du löschen möchtest, und drücke dann <btn id='Design.DeleteDoors'/>."
		Title = "Türen löschen"
	}
	DeleteShortcut
	{
		
		Tip = "Im Schiffs- oder im Lackierungseditor kannst du <btn id='Design.EraseCursorMode'/> gedrückt halten, um vorübergehend in den Löschmodus zu wechseln."
		Title = "Teile und Aufkleber entfernen"
	}
	DesigningAsteroids
	{
		
		Tip = "Im Kreativmodus kannst du Asteroiden genauso entwerfen wie Schiffe. <btn id='General.UIAltClick'/> auf die „Neues Schiff“-Schaltfläche und wähle „Asteroid“ oder „Megaroid“ aus. <gray>(Megaroiden sind wirklich große Asteroiden.)</gray>"
		Title = "Asteroiden gestalten"
	}
	DirectControlBindings
	{
		
		Tip1 = "Für den „Direkte Steuerung“-Modus kannst du anpassen, welche Maustaste verwendet wird, um die einzelnen Waffen abzufeuern. Wähle die Waffe(n), öffne ihr <img name='hamburger_up'/>-Menü und wähle <white>Tastenbelegung für direkte Steuerung</white>."
		Tip2 = "Du kannst Waffen auch so einrichten, dass sie <white>Von KI gesteuert</white> bleiben, selbst wenn du den „Direkte Steuerung“-Modus verwendest."
		Title = "Direkte Steuerung: Waffen-Tastenbelegungen"
	}
	DirectControlSettings
	{
		
		Tip = "Der „Direkte Steuerung“-Modus verfügt über zahlreiche Einstellungen, die du anpassen kannst, um die Steuerung und die Kamera auf deine Bedürfnisse anzupassen. Du kannst diese unter <good>Einstellungen -> Spiel -> „Direkte Steuerung“-Modus</good> und <good>Einstellungen -> Kamera -> „Direkte Steuerung“-Modus</good> verändern."
		Title = "Einstellungen für die direkte Steuerung"
	}
	DirectControlSuppress
	{
		
		Tip = "Wenn du dich im „Direkte Steuerung“-Modus befindest, kannst du diesen <i>vorübergehend</i> deaktivieren, indem du <btn id='Game.SelectParts'/> gedrückt hältst. Auf diese Weise hast du die Möglichkeit, das Schiff und deine Ansicht normal zu steuern. Wenn du <btn id='Game.SelectParts'/> wieder loslässt, kehrst du zurück in den „Direkte Steuerung“-Modus."
		Title = "„Direkte Steuerung“-Modus unterdrücken"
	}
	DisableAutoDoors
	{
		
		Tip = "Sobald du neue Räume zu deinem Schiff hinzufügst, werden standardmäßig automatisch Türen hinzugefügt. Wenn du das nicht möchtest, kannst du <btn id='Design.NoAutoDoors'/> gedrückt halten, um das Hinzufügen von Türen vorübergehend zu verhindern. Unter <good>Einstellungen -> Schiffseditor -> Türen automatisch hinzufügen</good> kannst du die Funktion auch komplett deaktivieren."
		Title = "Automatische Türen deaktivieren"
	}
	DisableEdgePan
	{
		
		Tip = "Ärgert es dich, wenn der Mauszeiger versehentlich den Rand des Bildschirms berührt, und die Ansicht dadurch verschoben wird? Wenn ja, kannst du das unter <good>Einstellungen -> Kamera -> Schwenken &amp; Drehen -> Randscrollen</good> deaktivieren."
		Title = Kamera-Randscrollen
	}
	DisableUI
	{
		
		Tip = "Du findest, dass deine Bildschirmfotos und GIFs ohne die Benutzeroberfläche schöner wären? Drücke <btn id='Game.ToggleUI'/>, um sie auszublenden. Durch erneutes Drücken kannst du sie wieder einblenden."
		Title = "UI deaktivieren"
	}
	Discord
	{
		
		Tip = "Cosmoteer besitzt eine lebendige und offene Community auf Discord! Um ihr beizutreten, klicke innerhalb des Titelmenüs auf<img name='hamburger_up'/>MEHR und wähle anschließend <good>Community -> Discord</good>."
		Title = ""
	}
	DisplayName
	{
		
		Tip = "In der Grundeinstellung erscheinst du im Multiplayer-Modus mit deinem Steam-Kontonamen. Aber unter <good>Einstellungen -> Verschiedenes -> Multiplayer -> Anzeigename</good> kannst du deinen Anzeigenamen ändern."
		Title = Multiplayer-Anzeigename
	}
	EditShipInfo
	{
		
		Tip = "Du kannst den Namen, den Designer (Autor), die Beschreibung und die Geschichte deines Schiffes bearbeiten. Öffne das <img name='hamburger_up'/>-Menü neben dem Namen deines Schiffes und wähle <white>Schiffsname/-informationen bearbeiten</white>. <gray>(Oder du klickst einfach den Namen selbst an!)</gray>"
		Title = "Schiffsinfo bearbeiten"
	}
	EngineRoomDistribution
	{
		
		Tip = "Maschinenräume können Batterien für angeschlossene Schubdüsen erhalten und verteilen. Das bedeutet, dass deine Crew deine Schubdüsen nicht wirklich erreichen können muss, solange deine Schubdüsen mit einem Maschinenraum verbunden sind, der erreicht werden <i>kann</i>."
		Title = "Energieverteilung über Maschinenraum"
	}
	FameLimit
	{
		
		Tip = "In den meisten Missionen gibt es ein „Ruhmlimit“. Wenn du bereits mehr <fame>Ruhm</fame> hast, als im Limit angegeben, erhältst du in der Mission keinen weiteren Ruhm. Absolviere Missionen auf höheren Ebenen, um weiter Ruhm zu verdienen."
		Title = Ruhmlimit
	}
	Feedback
	{
		
		Tip = "Wir freuen uns sehr über dein Feedback! Drücke jederzeit auf <btn id='General.SubmitFeedback'/>, um uns deine Ideen, Beschwerden, Fehlermeldungen oder jede Art von Feedback zu schicken. Vielen Dank!"
		Title = "Feedback senden"
	}
	FillDoors
	{
		
		Tip = "Möchtest du an jeder möglichen Stelle eine Tür einbauen? Halte <btn id='Design.FillAllDoors'/> gedrückt, während du eine Tür hinzufügst, um <i>überall</i> Türen hinzuzufügen."
		Title = "Alle Türen ausfüllen"
	}
	FlightDirection
	{
		
		Tip = "In der Grundeinstellung fliegen deine Schiffe immer so, als ob „Oben“ im Schiffseditor die Vorwärtsrichtung wäre. Du kannst die Vorwärtsrichtung ändern, indem du das <img name='hamburger_up'/>-Menü neben dem Namen deines Schiffs öffnest oder auf der rechten Seite des Schiffseditors auf die kleine Nach-Oben-Taste klickst."
		Title = Flugrichtung
	}
	ForceStartConstruction
	{
		
		Tip = "Normalerweise kannst du einen Bauvorgang nicht beginnen, wenn du nicht genug Ressourcen hast, um ihn auch zu beenden. Du kannst aber <btn id='Design.ConfirmPartialBuild'/> gedrückt halten, um den Baustart trotzdem zu erzwingen. Deine Crew kann die Konstruktion dann zwar nicht abschließen, aber sie wird so viel bauen wie möglich."
		Title = "Baustart erzwingen"
	}
	GameSpeedFramerate
	{
		
		Tip = "Wenn du die Spielgeschwindigkeit auf 8x erhöhst, bedeutet das, dass deine CPU 8-mal so viel Arbeit leisten muss wie bei 1-facher Geschwindigkeit. Das kann zu einer schlechten Bildfrequenz (FPS) führen, vor allem im späteren Spielverlauf. Du solltest in Betracht ziehen, die Spielgeschwindigkeit zu verringern, um die Bildfrequenz zu verbessern."
		Title = "Spielgeschwindigkeit & Bildfrequenz"
	}
	Hamburgers
	{
		
		FunFact = "<gray>(Fun Fact: Entwickler von Spielen und Apps nennen diese Art der Menüs gerne „Hamburger-Menüs“.)</gray>"
		Tip = "Cosmoteer verfügt über zahlreiche kleine <img name='hamburger_up'/> und <img name='hamburger_down'/>-Menüs. Halte Ausschau nach ihnen! Wenn du sie anklickst, erhältst du Zugriff auf viele erweiterte oder „versteckte“ Funktionen."
		Title = Hamburger-Menüs
	}
	HiringPriority
	{
		
		Tip1 = "Jede Crewrolle verfügt über eine „Einstellungspriorität“ zwischen <bad>1</bad> und <good>10</good>. Wenn du neue Crewmitglieder anheuerst oder auf dein Schiff transferierst, werden sie automatisch in die Rolle mit der höchsten Einstellungspriorität aufgenommen, in der noch Platz ist."
		Tip2 = "Wenn du die Option <white>Automatisch aus Rollen mit geringerer Priorität auffüllen</white> aktivierst, werden leere Crewplätze automatisch mit Crewmitgliedern aus anderen Rollen mit geringerer Einstellungspriorität gefüllt."
		Title = Einstellungspriorität
	}
	Hotkeys
	{
		
		Tip = "Zahlreiche Schaltflächen der Benutzeroberfläche verfügen über „Schnelltasten“, die du drücken kannst, anstatt die Schaltfläche anzuklicken. Diese Schnelltasten werden fast immer im Tooltip angezeigt, wenn du mit dem Mauszeiger über die Schaltfläche fährst, und sie können auch unter <good>Einstellungen -> Steuerung</good> angepasst werden."
		Title = Schnelltasten
	}
	LauncherTypes
	{
		
		Tip = "Den Raketen- oder Minentyp, den ein Werfer verschießt, kannst du jederzeit ändern – auch während eines Kampfes. Nutze dies, um deinen Feind mit ein paar unerwarteten Nuklearwaffen zu überraschen!"
		Title = Werfertypen
	}
	LoadBlueprints
	{
		
		Tip1 = "Du kannst die Baupläne eines anderen gespeicherten Schiffsdesigns in dein aktuelles Schiff laden. Öffne im Schiffseditor das <img name='hamburger_up'/>-Menü und klicke dann entweder:"
		Tip2 = "<b>Schiff laden (bestehendes ersetzen)</b> – hiermit werden die bestehenden Baupläne deines Schiffs vollständig durch die des gespeicherten Schiffsdesigns ersetzt."
		Tip3 = "<b>Schiff laden (in bestehendes einfügen)</b> – hiermit fügst du die Baupläne des gespeicherten Schiffs an einem Ort deiner Wahl in dein bestehendes Schiff ein."
		Title = "Baupläne laden"
	}
	LoadPaint
	{
		
		Tip = "Du kannst das Lackierungs-/Aufkleberdesign eines anderen gespeicherten Schiffs in dein aktuelles Schiff laden. Öffne im Lackierungseditor das <img name='hamburger_up'/>-Menü und klicke anschließend auf <white>Lackierungsdesign laden</white>."
		Title = "Lackierung laden"
	}
	LoadShapeFromPNG
	{
		
		Tip1 = "Du kannst externe Grafikprogramme wie MS Paint, Paint.NET, Gimp oder Photoshop verwenden, um die Umrisse der Basisform für deine Schiffe zu erstellen ..."
		Tip2 = "1. Speichere zunächst mithilfe eines externen Grafikprogramms deiner Wahl eine <b>weiß-auf-schwarz</b>-PNG-Bilddatei, die die allgemeine Form deines Schiffs darstellt. Weiße Pixel werden als Korridorkacheln interpretiert und schwarze Pixel als leerer Raum."
		Tip3 = "2. Öffne dann im Schiffseditor das <img name='hamburger_up'/>-Menü und wähle <white>Form von PNG laden</white>. Wähle die PNG-Bilddatei aus, die du gespeichert hast."
		Tip4 = "3. Die aktuellen Baupläne deines Schiffs werden mit den Bauplänen aus deiner PNG-Bilddatei ersetzt. Stelle dein Schiff fertig, indem du Waffen, Schubdüsen, Verteidigungsanlagen und andere Teile hinzufügst, die du für sinnvoll hältst."
		Title = "Form von PNG laden"
	}
	MidpointFocus
	{
		
