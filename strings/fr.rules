Tutorials
{
	
	AbandonedShips
	{
		
		Dialog = "<good>Choisissez ensuite combien de membres d'équipage</good> seront transférés vers le vaisseau abandonné (vous devrez peut-être sélectionner le vaisseau dans la liste déroulante à droite si vous disposez déjà d'une flotte ou si plusieurs vaisseaux abandonnés se trouvent à proximité)."
		Discover = "Vous découvrirez de temps à autre des vaisseaux qui ont été <good>abandonnés</good> par leur équipage.\n\nLes composants de ces vaisseaux peuvent être <good>récupérés pour obtenir des ressources</good> comme avec toute épave, mais vous pouvez aussi en <good>prendre le contrôle</good> afin de les ajouter à votre flotte."
		StartTransfer = "Cliquez sur le bouton <white>Commencer le transfert</white> pour y envoyer votre équipage. Une fois à bord, il en prendra le contrôle !"
		Title = "Vaisseaux abandonnés"
		Transfer = "Pour <good>prendre le contrôle</good> d'un vaisseau abandonné, cliquez sur le petit bouton <img name='hamburger_up'/> et sélectionnez l'option <white>Transférer équipage</white>."
	}
	AdvancedCommands
	{
		
		Attack = "La <good>commande Attaquer</good> (<btn id='Game.Attack'/>) vous permet d'attaquer la grande majorité des vaisseaux, y compris le vôtre ou ceux de vos alliés. Vous pouvez également l'utiliser pour ajuster rapidement l'angle et la distance d'un ordre d'attaque en cours."
