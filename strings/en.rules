	}
	PaintSchemes
	{
		Title = "Paint Schemes"
		Tip1 = "\"Paint Schemes\" are saved sets of paint colors and textures that can be shared between ships."
		Tip2 = "To save a ship's current colors and texture, open the <img name='hamburger_up'/> menu in the Paint Editor and then select <white>Save Paint Scheme</white>. To load that paint scheme on a different ship, open its <img name='hamburger_up'/> menu and select <white>Load Paint Scheme</white>."
	}

	// Crew
	CrewCustomization
	{
		Title = "Crew Customization"
		Tip = "The names and appearances of all your crew can be customized! Open the Crew Roster and then <white>double-click</white> on the crew member who you wish to customize."
