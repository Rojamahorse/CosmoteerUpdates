	}
	RoleOrder
	{
		Title = "Crew Role Order"
		Tip = "You can change the order in which your crew roles are listed by dragging-and-dropping them. This has no effect on how your crew function but can be useful for organization."
	}
	SharingRoles
	{
		Title = "Sharing Roles"
		Tip1 = "Once you have perfected your ship's crew roles, you may want to use those same roles with your other ships. There are several ways of doing that:"
		Tip2 = "1. You can load the roles from any saved ship design by opening the <img name='hamburger_up'/> menu in the Crew Management screen and selecting <white>Load Ship Roles</white>."
		Tip3 = "2. From that same <img name='hamburger_up'/> menu, you can also <white>Copy Roles</white> from one ship and then <white>Paste Roles</white> into another ship."
		Tip4 = "3. And lastly, you can select <white>Save As My Default Roles</white>. Any new ship designs you start will use those roles by default."
	}
	HiringPriority
	{
		Title = "Hiring Priority"
		Tip1 = "Every crew role has a \"Hiring Priority\" between <bad>1</bad> and <good>10</good>. When you hire or transfer new crew to your ship, they will automatically join the role with the highest Hiring Priority that has room."
		Tip2 = "If you enable the <white>Auto-Fill From Lower Priority Roles</white> option, then any empty crew slots will be automatically filled by transferring crew from roles with a lower Hiring Priority."
	}
	CrewResourceTransfer
	{
		Title = "Crew & Resource Transfer"
		Tip = "You can transfer crew and resources between your ships. Click the <img name='hamburger_up'/> menu next to your ship's name (or in the Crew or Resource Management screens) and then select <white>Transfer Crew / Resources</white>."
	}

	// Resources
	ResourceCollectionTypes
	{
		Title = "Resource Collection Types"
		Tip = "Do you already have enough of a certain kind of resource? <btn id='General.UIAltClick'/> on the Gather/Salvage/Mine button and unselect the resources that you no longer want. Your crew will no longer gather, salvage, or mine those types of resources."
	}
	ResourceLimits
	{
		Title = "Resource Limits"
		Tip1 = "You can limit how many resources your ship will store of any specific type. <btn id='General.UIAltClick'/> on the Gather/Salvage/Mine button and select <white>Set Resource Limits</white>."
		Tip2 = "When a limit has been set for a specific kind of resource, your crew won't gather, your factories won't produce, and you won't be able to buy, more than that limit."
	}
	MovingResources
	{
		Title = "Moving Resources"
		Tip = "When using the Resource Management screen to move resources, you can hold <btn id='Resources.Move1'/> to pick up or drop 1 resource at a time instead of the whole stack. Hold <btn id='Resources.Move5'/> for 5 at a time."
	}
	ResourceEjection
	{
		Title = "Resource Ejection"
		Tip = "To eject all or a specific number of resources, <btn id='General.UIAltClick'/> on its button along the bottom of the Resource Management screen and choose one of the <white>Eject</white> options."
	}

	// Trade
	Trade5x
	{
		Title = "Trading 5x"
		Tip = "While trading or transferring resources, you can hold <btn id='Resources.Transfer5'/> to move 5 times as many resources per click."
	}
	TradeAmount
	{
		Title = "Trading Exact Amounts"
		Tip1 = "When trading or transferring resources, you can click on the number and type in the exact amount you wish to buy, sell, or transfer. A positive number means that you want to buy (or transfer left to right). A negative number means that you want to sell (or transfer from right to left)."
