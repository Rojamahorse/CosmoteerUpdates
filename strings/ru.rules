Tips
{

	Accessibility
	{

		Tip = "Если у вас возникают проблемы с тем, чтобы отличить значки дружественных и вражеских кораблей, вы можете изменить их цвета в меню <good>Настройки > Специальные настройки</good>. Там же найдутся дополнительные настройки, которые сделают Cosmoteer более комфортной игрой для игроков с чувствительным зрением."
		Title = "Дальтонизм и специальные настройки"
	}
	AdvancedCareerMode
	{

		Tip = "В режиме «Карьера» есть несколько продвинутых опций, с помощью которых вы сможете изменить игру по своему вкусу, например мгновенное строительство кораблей или раскрытие всей звездной системы. Их можно включить, нажав <good>Дополнительные параметры игры</good> при запуске новой игры в режиме «Карьера» или в разделе <good>Настройки -> Текущая игра -> Продвинутые настройки игры</good>, когда игра уже идет."
		Title = "Дополнительные настройки режима «Карьера»"
	}
	AdvancedCommands
	{

		Tip = "Если вы не видите кнопки расширенного управления кораблём, их можно включить в разделе <good>Настройки -> Игровой процесс -> Показывать дополнительное управление кораблём</good>."
		Title = "Продвинутое управление кораблём"
	}
	AreYouSure
	{

		Tip = "Многие запросы подтверждения в духе <i>«Вы точно хотите...?»</i> можно пропустить, удерживая <btn id='General.SuppressConfirmation'/>."
		Title = "Уверены?"
	}
	AttackDefaults
	{

		Tip = "Устали от необходимости задавать дистанцию и вращение корабля каждый раз, когда атакуете врага? Откройте меню «<img name='hamburger_up'/>» рядом с названием своего корабля и нажмите <good>Задать настройки атаки по умолчанию</good>. Ваши текущие дистанция, вращение и угол атаки сохранятся на все последующие атаки."
		Title = "Настройки атак по умолчанию"
	}
	AutoSave
	{

		Tip = "По умолчанию Cosmoteer сохраняет текущую игру каждые 10 минут и хранит 10 файлов автосохранений. Файлы более старых автосохранений автоматически удаляются. Вы можете изменить интервал и допустимое количество файлов автосохранений в меню <good>Настройки -> Автосохранение</good>."
		Title = Автосохранения
	}
	BattleHelper
	{

		Tip = "В «Песочнице» вы можете использовать «Боевого помощника» для подготовки и проведения сражений между двумя противоборствующими флотами кораблей. Вы можете получить доступ к боевому помощнику из меню «<img name='hamburger_down'/>»."
		Title = "Боевой помощник"
	}
	BlueprintMode
	{

		Tip = "Вы можете использовать «Режим чертежа», чтобы внести временные, конструктивно некорректные или недоступные вам по финансам изменения в конструкцию вашего корабля, прежде чем приступить к ним на деле. Нажмите кнопку <white>Режим чертежа</white> в левом нижнем углу редактора кораблей, чтобы включить режим чертежа."
		Title = "Режим чертежа"
	}
	ChangingDifficulty
	{

		Tip = "Вы можете изменить уровень сложности в режиме карьеры в любой момент, а не только при запуске новой игры. Чтобы изменить уровень сложности, перейдите в <good>Настройки -> Текущая игра</good>."
		Title = "Изменение уровня сложности"
	}
	Chatting
	{

		Tip = "В мультиплеере вы можете отправлять текстовые сообщения другим игрокам, нажав <btn id='Game.Chat'/>."
		Title = "Чат мультиплеера"
	}
