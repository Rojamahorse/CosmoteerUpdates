Tips
{
	
	Accessibility
	{
		
		Tip = "Caso esteja com dificuldades para distinguir os ícones das naves aliadas e inimigas, você pode alterar as cores deles em <good>Configurações -> Acessibilidade</good>. Também há algumas configurações adicionais para facilitar a visualização de Cosmoteer."
		Title = "Daltonismo e configurações de acessibilidade"
	}
	AdvancedCareerMode
	{
		
		Tip = "O modo Carreira tem diversas opções avançadas que você pode alterar para personalizar o jogo à sua maneira, como tornar instantânea a construção de naves ou revelar o sistema estelar inteiro. Você pode ativá-las clicando em <good>Opções Avançadas de Jogo</good> ao começar uma nova carreira ou em <good>Configurações -> Jogo Atual -> Opções Avançadas de Jogo</good> durante um jogo."
		Title = "Opções avançadas do modo Carreira"
	}
	AdvancedCommands
	{
		
		Tip = "Caso não veja esses botões de comandos avançados da nave, você pode ativá-los em <good>Configurações -> Jogabilidade -> Exibir Comandos Avançados da Nave.</good>"
		Title = "Comandos avançados da nave"
	}
	AreYouSure
	{
		
		Tip = "Muitas das confirmações estilo <i>\"Você quer mesmo...?\"</i> podem ser puladas ao segurar <btn id='General.SuppressConfirmation'/>."
		Title = "Você quer mesmo?"
	}
	AttackDefaults
	{
		
		Tip = "Cansado de ter que definir a distância e a rotação da sua nave toda vez que vai atacar um inimigo? Abra o menu <img name='hamburger_up'/> perto do nome da sua nave e clique em <good>Salvar Padrões de Ataque</good>. Sua distância atual, a rotação e o ângulo de ataque serão lembrados na próxima vez que você atacar algo."
		Title = "Definindo padrões de ataque"
	}
	AutoSave
	{
		
		Tip = "Por padrão, Cosmoteer vai salvar automaticamente seu jogo atual a cada 10 minutos e vai manter seus 10 últimos salvamentos. Os salvamentos mais antigos serão excluídos automaticamente. Você pode alterar a duração e a quantidade de salvamentos que quer manter em <good>Configurações -> Salvamento automático</good>."
		Title = "Salvamentos automáticos"
	}
	BattleHelper
	{
		
		Tip = "No modo Criativo, você pode usar o Ajudante de Batalha para configurar e executar batalhas entre duas frotas de naves. Você pode acessar o Ajudante de Batalha a partir do menu <img name='hamburger_down'/>."
		Title = "Ajudante de batalha"
	}
	BlueprintMode
	{
		
		Tip = "Você pode usar o \"Modo Planta\" para fazer alterações temporárias, ilegais ou fora do orçamento na sua nave antes de realmente executar tais mudanças. Clique no botão <white>Modo Planta</white> no canto inferior esquerdo no Editor de Naves para habilitar o modo Planta."
		Title = "Modo Planta"
	}
	ChangingDifficulty
	{
		
		Tip = "Você pode alterar o nível de dificuldade no modo Carreira a qualquer momento, não apenas no começo de um novo jogo. Para alterar a dificuldade, vá para <good>Configurações -> Jogo Atual</good>."
		Title = "Alterando a dificuldade"
	}
	Chatting
	{
		
		Tip = "No multijogador, você pode enviar mensagens de chat para os outros jogadores apertando <btn id='Game.Chat'/>."
		Title = "Chat multijogador"
	}
	CollisionAvoidance
	{
		
		Tip = "As naves buscam contornar obstáculos grandes para evitar colidir com eles. Caso não goste disso, você pode segurar <btn id='Game.InvertCollisionAvoidance'/> enquanto dá um comando de movimentação para desabilitar a prevenção de colisão, ou desligue isso permanentemente em <good>Configurações -> Jogabilidade -> Comandos da Nave -> Habilitar Prevenção de Colisão</good>."
		Title = "Prevenção de colisão"
	}
	ControlGroups
	{
		
		Tip1 = "Você pode colocar uma ou mais naves em um \"grupo de controle\" selecionando-as e apertando <white>Ctrl + F1-F8</white>. Você pode selecionar todas as naves desse grupo apertando <white>F1-F8</white>."
		Tip2 = "Da mesma forma, você pode colocar uma ou mais armas ou outras peças da nave em grupos de controle selecionando-as e apertando <white>Ctrl + 0-9</white>. Você pode selecionar todas as peças desse grupo apertando <white>0-9</white>."
		Title = "Grupos de controle"
	}
	CrewCustomization
	{
		
		Tip = "Os nomes e as aparências de toda sua tripulação podem ser personalizados! Abra a Lista de Tripulação e dê um <white>clique duplo</white> no membro que você quer personalizar."
		Title = "Personalização de tripulação"
	}
	CrewFavorite
	{
		
		Tip = "Você gosta mais de alguns da sua tripulação do que de outros? Dê um clique com o <white>botão direito</white> neles na Lista de Tripulação e marque-os como \"favoritos\". Dessa forma, você sempre saberá onde eles estão!"
		Title = "Tripulantes favoritos"
	}
	CrewResourceTransfer
	{
		
		Tip = "Você pode transferir recursos e tripulação entre suas naves. Clique no menu <img name='hamburger_up'/> perto do nome da sua nave (ou nas telas da Tripulação ou de Gerenciamento de Recursos) e selecione <white>Transferir Tripulação / Recursos</white>."
		Title = "Transferência de recursos e tripulação"
	}
	CtrlSelectsParts
	{
		
		Tip = "Caso não consiga selecionar armas e outras peças da nave simplesmente clicando nelas, você precisa segurar <btn id='Game.SelectParts'/> ou desabilitar essa configuração em <good>Configurações -> Jogabilidade -> Requisitar Ctrl para Selecionar Peças</good>."
		Title = "Requisitar Ctrl para selecionar peças"
	}
	CtrlTargeting
	{
		
		Tip1 = "Você pode segurar <btn id='Game.SelectParts'/> para mirar suas armas nas naves inimigas sem precisar atacá-las."
		Tip2 = "Você também pode segurar <btn id='Game.SelectParts'/> para forçar a mira de suas armas em naves aliadas."
		Title = "Mirar com Ctrl"
	}
	DecalLayers
	{
		
		Tip = "Você pode clicar no ícone de \"olho\" para ocultar ou exibir as camadas de adesivos. Arraste e solte camadas de adesivos para alterar a ordem."
		Title = "Camadas de adesivos"
	}
	DeleteDoors
	{
		
		Tip = "Caso queira apagar várias portas de uma só vez, use a ferramenta de seleção para selecionar todas as peças cujas portas você quer apagar, e então aperte <btn id='Design.DeleteDoors'/>."
		Title = "Apagando portas"
	}
	DeleteShortcut
	{
		
		Tip = "Você pode segurar <btn id='Design.EraseCursorMode'/> para alterar temporariamente para o modo de exclusão no Editor de Naves ou no Editor de Pintura."
		Title = "Removendo peças e adesivos"
	}
	DesigningAsteroids
	{
		
		Tip = "No modo Criativo, você pode fazer o design de asteroides assim como o de naves. <btn id='General.UIAltClick'/> no botão Nova Nave e selecione Asteroide ou Megaroide. <gray>Megaroides são asteroides muito grandes.</gray>"
		Title = "Designando asteroides"
	}
	DirectControlBindings
	{
		
		Tip1 = "No modo Controle Direto, você pode personalizar qual botão do mouse é usado para disparar cada arma. Selecione a(s) arma(s), abra o menu <img name='hamburger_up'/> dela(s) e selecione <white>Afixar para Controle Direto</white>."
		Tip2 = "Você também pode definir as armas para serem <white>controladas por IA</white> mesmo usando o modo Controle Direto."
		Title = "Afixar armas no controle direto"
	}
	DirectControlSettings
	{
		
		Tip = "O modo Controle Direto tem muitas configurações que você pode alterar para personalizar os controles e a câmera de acordo com suas preferências. Você pode alterá-las em <good>Configurações -> Jogabilidade -> Modo Controle Direto</good> e em <good>Configurações -> Câmera -> Modo Controle Direto</good>."
		Title = "Configurações de controle direto"
	}
	DirectControlSuppress
	{
		
		Tip = "Durante o modo Controle Direto, você pode desabilitá-lo <i>temporariamente</i> ao segurar <btn id='Game.SelectParts'/>, possibilitando que você comande a nave e controle a visualização normalmente. Soltar <btn id='Game.SelectParts'/> vai levá-lo de volta ao modo Controle Direto."
		Title = "Suprimindo o modo controle direto"
	}
	DisableAutoDoors
	{
		
		Tip = "Ao adicionar novas salas à sua nave, as portas serão adicionadas automaticamente por padrão. Caso não goste disso, você pode segurar <btn id='Design.NoAutoDoors'/> para impedir temporariamente que elas sejam adicionadas, ou você pode desativar essa função em <good>Configurações -> Editor de Naves -> Adicionar portas automaticamente</good>."
		Title = "Desabilitando portas automáticas"
	}
	DisableEdgePan
	{
		
		Tip = "Você se incomoda quando o cursor do mouse toca as bordas da tela e faz a câmera se mover? Se sim, você pode desligar isso em <good>Configurações -> Câmera -> Movimento e Rotação -> Panorâmica na Borda</good>."
		Title = "Movimento de panorâmica na borda"
	}
	DisableUI
	{
		
		Tip = "Acha que suas capturas de tela e GIFs ficariam melhores sem a interface de usuário? Aperte <btn id='Game.ToggleUI'/> para ocultá-la. Aperte de novo para voltar a exibi-la."
		Title = "Desabilitando a interface"
	}
	Discord
	{
		
		Tip = "Cosmoteer tem uma comunidade receptiva e vibrante no Discord! Para entrar, clique em <img name='hamburger_up'/>MAIS na tela inicial e selecione <good>Comunidade -> Discord</good>."
		Title = ""
	}
	DisplayName
	{
		
		Tip = "Por padrão, você vai para o multijogador usando o nome da sua conta Steam, mas você pode alterar seu nome de exibição em <good>Configurações -> Diversos -> Multijogador -> Exibir nome</good>."
		Title = "Nome de exibição no multijogador"
	}
	EditShipInfo
	{
		
		Tip = "Você pode editar o nome da sua nave, o designer (autor), a descrição e a história. Abra o menu <img name='hamburger_up'/> perto do nome da sua nave e selecione <white>Editar nome/informações da nave</white> <gray>(ou você pode só clicar no nome mesmo)</gray>."
		Title = "Editando as informações da nave"
	}
	EngineRoomDistribution
	{
		
		Tip = "A sala dos motores pode receber e distribuir baterias de energia para qualquer propulsor conectado, de forma que sua tripulação não precise alcançar seus propulsores, contanto que seus eles estejam conectados a uma sala dos motores que <i>possa</i> ser alcançada."
		Title = "Distribuição de energia da sala dos motores"
	}
	FameLimit
	{
		
		Tip = "A maioria das missões tem um \"limite de fama\". Caso já tenha mais <fame>Fama</fame> do que o limite, a missão não concederá mais dela. Conclua missões de nível mais alto para continuar a obter fama."
		Title = "Limite de fama"
	}
	Feedback
	{
		
		Tip = "Adoraríamos receber seus comentários! Aperte <btn id='General.SubmitFeedback'/> a qualquer momento para nos enviar suas ideias, reclamações, relatos de erros ou qualquer tipo de comentário. Agradecemos!"
		Title = "Enviando comentários"
	}
	FillDoors
	{
		
		Tip = "Quer uma porta em cada local possível? Segure <btn id='Design.FillAllDoors'/> enquanto adiciona uma porta para adicionar portas <i>em toda parte</i>."
		Title = "Preenchendo todas as portas"
	}
	FlightDirection
	{
		
		Tip = "Por padrão, suas naves sempre voarão como se \"cima\" no Editor de Naves fosse para frente. Você pode alterar qual direção é \"para frente\" abrindo o menu <img name='hamburger_up'/> perto do nome da sua nave ou clicando na setinha para cima no lado direito do Editor de Naves."
		Title = "Direção do voo"
	}
	ForceStartConstruction
	{
		
		Tip = "Normalmente, você não pode iniciar uma construção se não tiver recursos para concluí-la. No entanto, você pode segurar <btn id='Design.ConfirmPartialBuild'/> para forçar o início da construção de qualquer forma. Sua tripulação não conseguirá concluí-la, mas fará o quanto for possível."
		Title = "Início forçado de construção"
	}
	GameSpeedFramerate
	{
		
		Tip = "Aumentar a velocidade de jogo para 8x vai exigir 8 vezes mais da sua CPU. Isso pode reduzir a taxa de quadros (FPS), especialmente em um jogo avançado. Considere reduzir a velocidade de jogo para melhorar a taxa de quadros."
		Title = "Velocidade de jogo e taxa de quadros"
	}
	Hamburgers
	{
		
		FunFact = "<gray>Curiosidade: desenvolvedores de apps e de jogos gostam de chamar isso de \"menu hambúrguer\".</gray>"
		Tip = "Cosmoteer tem vários menuzinhos <img name='hamburger_up'/> e <img name='hamburger_down'/>. Fique de olho! Clicar neles vai conceder acesso a várias funções avançadas ou \"ocultas\"."
		Title = "Menus hambúrguer"
	}
	HiringPriority
	{
		
		Tip1 = "Cada cargo de tripulante tem uma \"prioridade de contratação\" entre <bad>1</bad> e <good>10</good>. Quando você contrata ou transfere um tripulante novo à sua nave, ele entra automaticamente no cargo com a prioridade de contratação mais alta que tenha vaga."
		Tip2 = "Se você habilitar a opção de <white>preenchimento automático a partir dos cargos de baixa prioridade</white>, qualquer espaço vago de tripulante será automaticamente preenchido por tripulantes de outros cargos com prioridade mais baixa."
		Title = "Prioridade de contratação"
	}
	Hotkeys
	{
		
		Tip = "Muitos botões da interface têm \"atalhos\" que você pode apertar em vez de clicar no botão. Esses atalhos são quase sempre exibidos como dicas ao colocar o cursor do mouse sobre o botão, e eles também podem ser personalizados em <good>Configurações -> Controles</good>."
		Title = Atalhos
	}
	LauncherTypes
	{
		
		Tip = "Você pode alterar qual tipo de míssil ou mina um lançador dispara a qualquer momento, inclusive durante uma batalha. Use isso para surpreender seu inimigo com algumas armas inesperadas!"
		Title = "Tipos de lançador"
	}
	LoadBlueprints
	{
		
		Tip1 = "Você pode carregar as plantas de outro design de nave salvo na sua nave atual. No Editor de naves, abra o menu <img name='hamburger_up'/> e clique em:"
		Tip2 = "<b>Carregar nave (substituir a existente)</b> - Isso vai substituir completamente as plantas existentes na sua nave por aquelas do design de nave salvo."
		Tip3 = "<b>Carregar nave (colar na existente)</b> - Isso vai adicionar as plantas da nave salva na sua nave atual em um local à sua escolha."
		Title = "Carregando plantas"
	}
	LoadPaint
	{
		
		Tip = "Você pode transferir pinturas/adesivos de outro design de nave salvo para a sua nave atual. No Editor de Pintura, abra o menu <img name='hamburger_up'/> e clique em <white>Carregar Design de Pintura</white>."
		Title = "Carregando pintura"
	}
	LoadShapeFromPNG
	{
		
		Tip1 = "Você pode usar programas de arte de terceiros, como MS Paint, Paint.NET, GIMP ou Photoshop para criar o rascunho da forma básica de suas naves..."
		Tip2 = "1. Primeiro, use algum programa de arte de terceiros para salvar um arquivo de imagem PNG <b>preto e branco</b> que represente a forma geral da sua nave. Os pixels brancos são interpretados como corredores, e os pretos como espaço vazio."
		Tip3 = "2. Depois, no Editor de Naves, abra o menu <img name='hamburger_up'/> e selecione <white>Carregar forma a partir de PNG</white>. Selecione o arquivo da imagem PNG que você salvou."
		Tip4 = "3. As plantas atuais da sua nave serão substituídas pelas plantas criadas no seu arquivo de imagem PNG. Finalize sua nave adicionando armas, propulsores, defesas e outras peças que achar adequado."
		Title = "Carregar forma a partir de PNG"
	}
	MidpointFocus
	{
		
		Tip = "Após atacar uma nave inimiga, você pode clicar no ícone de \"olho\" para focar sua visualização no ponto intermediário entre sua nave e o alvo."
		Title = "Ícone de ponto intermediário do foco"
	}
	MirrorModeSettings
	{
		
		Tip = "Você já deve saber que pode habilitar o \"Modo Espelho\" para fazer alterações simétricas no design da sua nave. Você também pode <btn id='General.UIAltClick'/> no botão do modo Espelho para alterar a direção e a quantidade de linhas de simetria."
		Title = "Configurações do modo Espelho"
	}
	MissionsHUD
	{
		
		Tip1 = "Por padrão, apenas as 3 missões mais próximas serão exibidas no canto superior esquerdo da sua tela. Você pode alterar essa quantidade em <good>Configurações -> Jogabilidade -> Sobreposições de informação´-> Mostrar missões próximas no HUD</good>."
		Tip2 = "Você também pode marcar uma missão como \"favorita\" ao <white>clicar com o botão direito</white> nela ou apertando no botão \"Favoritar\" no registro de carreira. Assim, a missão será sempre exibida mesmo com outras missões mais próximas."
		Title = "HUD das missões"
	}
	ModAutoDisable
	{
		
		Tip = "Ao atualizar Cosmoteer, todos mods instalados <b>podem</b> ser automaticamente desabilitados caso não sejam compatíveis com a nova atualização de Cosmoteer. Caso não queira que os mods sejam desabilitados automaticamente, desative em <good>Configurações -> Diversos -> Desabilitar mods ao atualizar</good>."
		Title = "Desabilitando mods automaticamente"
	}
	MovingResources
	{
		
		Tip = "Ao usar a tela de Gerenciamento de Recursos para mover recursos, você pode segurar <btn id='Resources.Move1'/> para pegar ou soltar 1 recurso por vez, e não o lote todo. Segure <btn id='Resources.Move5'/> para mover 5 por vez."
		Title = "Movendo recursos"
	}
	ObjectiveFocus
	{
		
		Tip = "Você pode clicar na maioria dos objetivos de missões para focar sua visualização no local para onde você precisa ir para concluir o objetivo."
		Title = "Focando em objetivos"
	}
	PaintSchemes
	{
		
		Tip1 = "\"Esquemas de pintura\" são conjuntos salvos de cores e texturas que podem ser compartilhados entre naves."
		Tip2 = "Para salvar as cores e texturas atuais de uma nave, abra o menu <img name='hamburger_up'/> no Editor de Pintura e selecione <white>Salvar Esquema de Pintura</white>. Para carregar esse esquema de pintura em uma nave diferente, abra o menu <img name='hamburger_up'/> e selecione <white>Carregar Esquema de Pintura</white>."
		Title = "Esquemas de pintura"
	}
	PartBrushes
	{
		
		Tip = "A maioria das peças de tamanho 1x1 pode ser usada com diversos tamanhos e formatos de \"pincel\". Clique no botão de seleção do pincel quando estiver disponível para ver todas as opções."
		Title = "Pincéis de peças"
	}
	PartRows
	{
		
		Tip = "Você pode aumentar a quantidade de peças visíveis no Editor de Naves em <good>Configurações -> Editor de Naves -> Linhas de Peças</good>."
		Title = "Linhas do editor de naves"
	}
	PipSettings
	{
		
