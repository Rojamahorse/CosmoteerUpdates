Tips
{

	Accessibility
	{

		Tip = "Si tienes problemas para distinguir entre los íconos de las naves aliadas y enemigas, puedes cambiarles los colores en <good>Ajustes -> Accesibilidad</good>. Allí también encontrarás ajustes adicionales para que Cosmoteer sea más ameno con quienes tengan una vista sensible."
		Title = "Ajustes de accesibilidad y daltonismo"
	}
	AdvancedCareerMode
	{

		Tip = "El modo de carreras tiene un puñado de opciones avanzadas que puedes modificar para ajustar el juego a tus preferencias, como hacer que la construcción de naves sea instantánea o revelar todo el sistema estelar. Puedes activar estas opciones haciendo clic en <good>Opciones avanzadas del juego</good> al iniciar una nueva partida de carrera o en <good>Ajustes -> Partida actual -> Opciones avanzadas del juego</good> mientras juegas."
		Title = "Opciones avanzadas del modo de carrera"
	}
	AdvancedCommands
	{

		Tip = "Si no puedes ver estos botones de comandos avanzados de la nave, entonces puedes activarlos en <good>Ajustes -> Experiencia de juego -> Mostrar comandos avanzados de la nave</good>."
		Title = "Comandos avanzados de la nave"
	}
	AreYouSure
	{

		Tip = "Muchas de las ventanas de confirmación emergentes del estilo <i>\"¿De verdad quieres...?\"</i> se pueden omitir manteniendo <btn id='General.SuppressConfirmation'/>."
		Title = "¿De verdad?"
	}
	AttackDefaults
	{

		Tip = "¿Te cansa tener que establecer la distancia y rotación de la nave cada vez que atacas un enemigo? Abre el menú <img name='hamburger_up'/> junto al nombre de tu nave y haz clic en <good>Guardar valores predeterminados de ataque</good>. El juego recordará la distancia, rotación y ángulo del ataque actual la próxima vez que ataques algo."
		Title = "Establecer valores predeterminados de ataque"
	}
	AutoSave
	{

		Tip = "Por defecto, Cosmoteer guardará la partida actual de forma automática cada 10 minutos y guardará los últimos 10 archivos de guardado automático. Los archivos de guardado automático más viejos se borrarán de forma automática. Puedes cambiar la duración y la cantidad de archivos de guardado automático en <good>Ajustes -> Guardado automático</good>."
		Title = "Guardado automático"
	}
	BattleHelper
	{

		Tip = "En el modo creativo, puedes usar el Ayudante de batalla para configurar y ejecutar batallas entre dos flotas de naves opositoras. Puedes acceder al Ayudante de batalla desde el menú <img name='hamburger_down'/>."
		Title = "Ayudante de batalla"
	}
	BlueprintMode
	{

		Tip = "Puedes usar el \"Modo de plano\" para realizar cambios temporales, ilegales o carísimos al diseño de la nave antes de comprometerte de verdad a dichos cambios. Haz clic en el botón del <white>Modo de plano</white> en la parte inferior izquierda del Editor de naves para activar el Modo de plano."
		Title = "Modo de plano"
	}
	ChangingDifficulty
	{

		Tip = "Puedes cambiar el nivel de dificultad en el modo de Carrera en cualquier momento mientras juegas, no solo al iniciar un nuevo juego. Para cambiar la dificultad, ve a <good>Ajustes -> Juego actual</good>."
		Title = "Cambiar dificultad"
	}
	Chatting
	{

		Tip = "En el modo multijugador, puedes enviar mensajes en el chat a otras personas presionando <btn id='Game.Chat'/>."
		Title = "Chat multijugador"
	}
	CollisionAvoidance
	{

		Tip = "A menudo, las naves intentarán navegar alrededor de los obstáculos grandes para evitar chocarlos. Si no te gusta, entonces puedes mantener presionado <btn id='Game.InvertCollisionAvoidance'/> mientras das una orden de movimiento para desactivar la prevención de colisiones o puedes detenerla de forma más permanente si desactivas <good>Ajustes -> Experiencia de juego -> Comandos de la nave -> Activar prevención de colisiones</good>."
		Title = "Prevención de colisiones"
	}
	ControlGroups
	{

		Tip1 = "Puedes colocar una o más de tus naves en un \"grupo de control\" seleccionándolas y luego presionando <white>Ctrl + F1-F8</white>. Luego puedes seleccionar todas las naves de ese grupo presionando <white>F1-F8</white>."
		Tip2 = "Así mismo, puedes colocar una o más armas individuales u otras piezas de la nave en grupos de control seleccionándolas y luego presionando <white>Ctrl + 0-9</white>. Luego puedes seleccionar todas las piezas del grupo presionando <white>0-9</white>."
		Title = "Grupos de control"
	}
	CrewCustomization
	{

		Tip = "¡Puedes personalizar los nombres y las apariencias de todos tus tripulantes! Abre la Lista de tripulación y luego <white>haz doble clic</white> sobre el tripulante que quieras personalizar."
		Title = "Personalización de tripulación"
	}
	CrewFavorite
	{

		Tip = "¿Algunos de tus tripulantes te importan más que los demás? Haz <white>clic derecho</white> sobre ellos en la Lista de tripulación y márcalos como \"favoritos\". ¡De esa forma podrás saber siempre dónde están!"
		Title = "Tripulantes favoritos"
	}
	CrewResourceTransfer
	{

		Tip = "Puedes transferir tripulantes y recursos entre las naves. Haz clic sobre el menú <img name='hamburger_up'/> junto al nombre de la nave (o en las pantallas de Gestión de tripulantes o recursos) y luego selecciona <white>Transferir tripulantes/recursos</white>."
		Title = "Transferencia de tripulantes y recursos"
	}
	CtrlSelectsParts
	{

		Tip = "Si no puedes seleccionar armas individuales y otras piezas de la nave al clic en ellas, entonces debes mantener <btn id='Game.SelectParts'/> o desactivar el ajuste en <good>Ajustes -> Experiencia de juego -> Solicitar Ctrl para seleccionar las piezas</good>."
		Title = "Solicitar Ctrl para seleccionar las piezas"
	}
	CtrlTargeting
	{

		Tip1 = "Puedes mantener presionado <btn id='Game.SelectParts'/> para enfocar tus armas sobre las naves enemigas sin tener que atacarlas primero."
		Tip2 = "También puedes mantener <btn id='Game.SelectParts'/> para forzar el enfoque de tus armas sobre las naves aliadas."
		Title = "Puntería con Ctrl"
	}
	DecalLayers
	{

		Tip = "Puedes hacer clic sobre el ícono del \"ojo\" para mostrar u ocultar las capas de calcomanías. Arrastra y suelta las capas de calcomanías para cambiar el orden de estas."
		Title = "Capas de calcomanías"
	}
	DeleteDoors
	{

		Tip = "Si quieres borrar muchas puertas al mismo tiempo, usa la herramienta de selección para seleccionar todas las partes cuyas puertas quieras borrar y luego presiona <btn id='Design.DeleteDoors'/>."
		Title = "Borrar puertas"
	}
	DeleteShortcut
	{

		Tip = "En el Editor de naves o el Editor de pintura, puedes mantener presionado <btn id='Design.EraseCursorMode'/> para cambiar de forma temporal al modo de borrado."
		Title = "Eliminar piezas y calcomanías"
	}
	DesigningAsteroids
	{

		Tip = "En el modo creativo, puedes diseñar asteroides al igual que puedes diseñar naves. <btn id='General.UIAltClick'/> sobre el botón de Nueva nave y selecciona Asteroide o Megaroide. <gray>(Los megaroides son asteroides muy grandes).</gray>"
		Title = "Diseñar asteroides"
	}
	DirectControlBindings
	{

		Tip1 = "Para el modo de control directo, puedes personalizar qué botón del mouse se usa para disparar cada arma. Selecciona la/s arma/s, abre su menú <img name='hamburger_up'/> y selecciona <white>Enlace de control directo</white>."
		Tip2 = "También puedes elegir que las armas sigan <white>controladas por la IA</white> incluso al usar el modo de control directo."
		Title = "Enlaces de control directo de armas"
	}
	DirectControlSettings
	{

		Tip = "El modo de control directo tiene muchos ajustes que puedes modificar para que los controles y la cámara se ajusten a tus preferencias. Puedes cambiarlos en <good>Ajustes -> Experiencia de juego -> Modo de control directo</good> y <good>Ajustes -> Cámara -> Modo de control directo</good>."
		Title = "Ajustes del control directo"
	}
	DirectControlSuppress
	{

		Tip = "Cuando estás en el modo de control directo, puedes desactivarlo <i>temporalmente</i> al mantener presionado <btn id='Game.SelectParts'/>, esto te permite comandar la nave y controlar la vista de forma normal. Al soltar <btn id='Game.SelectParts'/> volverás al modo de control directo."
		Title = "Suprimir el modo de control directo"
	}
	DisableAutoDoors
	{

		Tip = "Al agregar nuevas habitaciones a la nave, las puertas se agregarán de forma automática por defecto. Si no te gusta, entonces puedes mantener presionado <btn id='Design.NoAutoDoors'/> para evitar de forma temporal que se agreguen o puedes desactivar la función por completo en <good>Ajustes -> Editor de naves -> Agregar puertas automáticamente</good>."
		Title = "Desactivar las puertas automáticas"
	}
	DisableEdgePan
	{

		Tip = "¿Te molesta cuando el cursor del mouse toca accidentalmente los bordes de la pantalla y se mueve la vista? Si es así, puedes desactivar eso en <good>Ajustes -> Cámara -> Ampliación de avance; rotar -> Avance por los bordes</good>."
		Title = "Avance por los bordes de cámara"
	}
	DisableUI
	{

		Tip = "¿Crees que las capturas de pantalla y GIFs serían más lindos sin la interfaz de usuario? Presiona <btn id='Game.ToggleUI'/> para ocultarla. Vuelve a presionar para mostrarla."
		Title = "Desactivar la IU"
	}
	Discord
	{

		Tip = "¡Cosmoteer tiene una comunidad dinámica y acogedora en Discord! Para unirte, haz clic en <img name='hamburger_up'/>MÁS en la pantalla de inicio y luego selecciona <good>Comunidad -> Discord</good>."
		Title = ""
	}
	DisplayName
	{

		Tip = "Por defecto, aparecerás en el modo multijugador con tu nombre de cuenta de Steam, pero puedes cambiar el nombre para mostrar en <good>Ajustes -> Varios -> Multijugador -> Nombre para mostrar</good>."
		Title = "Nombre para mostrar en multijugador"
	}
	EditShipInfo
	{

		Tip = "Puedes editar el nombre, el diseñador (autor), la descripción y conocimiento popular de la nave. Abre el menú <img name='hamburger_up'/> junto al nombre de la nave y selecciona <white>Editar nombre/información de la nave</white>. <gray>(¡O puedes solo hacer clic sobre el nombre!)</gray>"
		Title = "Editar la información de la nave"
	}
	EngineRoomDistribution
	{

		Tip = "Las salas de máquinas pueden recibir y distribuir baterías de energía a los propulsores conectados, lo que significa que la tripulación no necesita acceder a los propulsores mientras estén conectados a una sala de máquinas que <i>sea</i> accesible."
		Title = "Distribución de energía de la sala de máquinas"
	}
	FameLimit
	{

		Tip = "La mayoría de las misiones tienen un \"límite de fama\". Si ya tienes más <fame>fama</fame> que el límite, la misión no te dará más fama. Completa misiones de mayor nivel para seguir consiguiendo fama."
		Title = "Límite de fama"
	}
	Feedback
	{

		Tip = "¡Nos encantaría recibir tus comentarios! Presiona <btn id='General.SubmitFeedback'/> en cualquier momento para enviarnos ideas, quejas, informes de errores o cualquier tipo de comentarios. ¡Gracias!"
		Title = "Enviar comentarios"
	}
	FillDoors
	{

		Tip = "¿Quieres que haya una puerta en cada ubicación posible? Mantén presionado <btn id='Design.FillAllDoors'/> mientras agregas una puerta para agregar puertas <i>en todos lados</i>."
		Title = "Colocar todas las puertas"
	}
	FlightDirection
	{

		Tip = "Por defecto, tus naves vuelan siempre como si \"arriba\" en el Editor de naves fuera la dirección de avance. Puedes cambiar en qué dirección se \"avanza\" abriendo el menú <img name='hamburger_up'/> junto al nombre de la nave o haciendo clic sobre la pequeña flecha hacia arriba en la parte derecha del Editor de naves."
		Title = "Dirección de vuelo"
	}
	ForceStartConstruction
	{

		Tip = "Normalmente, no puedes comenzar una construcción si no tienes los recursos suficientes para terminarla. Sin embargo, puedes mantener presionado <btn id='Design.ConfirmPartialBuild'/> para forzar el comienzo de la construcción de todos modos. La tripulación no podrá terminarla, pero hará todo lo que pueda."
		Title = "Forzar el comienzo de la construcción"
	}
	GameSpeedFramerate
	{

		Tip = "Aumentar la velocidad del juego a x8 significa que tu CPU tiene que hacer 8 veces más trabajo que en la velocidad x1. Esto puedes causar pocos cuadros por segundos (FPS), más que nada en la parte final del juego. Considera reducir la velocidad del juego para mejorar los cuadros por segundo."
		Title = "Velocidad del juego y cuadros por segundo"
	}
	Hamburgers
	{

		FunFact = "<gray>(Dato curioso: A los desarrolladores de juegos y aplicaciones les gusta llamarlos \"menús de hamburguesa\").</gray>"
		Tip = "Cosmoteer tiene muchos pequeños menús <img name='hamburger_up'/> y <img name='hamburger_down'/>. ¡No los pierdas de vista! Si haces clic sobre ellos tendrás acceso a varias funciones avanzadas u \"ocultas\"."
		Title = "Menús de hamburguesa"
	}
	HiringPriority
	{

		Tip1 = "Cada rol de la tripulación tiene una \"prioridad de contratación\" entre <bad>1</bad> y <good>10</good>. Cuando contratas o transfieres nuevos tripulantes a la nave, se unirán de forma automática al rol con la mayor prioridad de contratación que tenga espacio."
		Tip2 = "Si activas la opción de <white>Autocompletar desde los roles de menor prioridad</white>, entonces cualquier espacio de tripulación vacío se completará de forma automática, transfiriendo tripulantes desde los roles con una menor prioridad de contratación."
		Title = "Prioridad de contratación"
	}
	Hotkeys
	{

		Tip = "Muchos botones de la interfaz de usuario tienen \"teclas de acceso rápido\" que puedes presionar en vez de hacer clic sobre el botón. Estas teclas de acceso rápido casi siempre se muestran en la descripción emergente al pasar el cursor del mouse por encima del botón y además se pueden personalizar en <good>Ajustes -> Controles</good>."
		Title = "Teclas de acceso rápido"
	}
	LauncherTypes
	{

		Tip = "Puedes cambiar el tipo de misil o mina que dispara un lanzador en cualquier momento, incluyendo durante la batalla. ¡Úsalo para sorprender a tus enemigos con algunos misiles nucleares inesperados!"
		Title = "Tipos de lanzadores"
	}
	LoadBlueprints
	{

		Tip1 = "Puedes cargarle los planos de otro diseño de nave guardado a tu nave actual. En el Editor de naves, abre el menú <img name='hamburger_up'/> y luego haz clic sobre alguna de las siguientes opciones:"
		Tip2 = "<b>Cargar nave (reemplazar la existente)</b>: Esto reemplazará por completo los planos de la nave existente con los del diseño de la nave guardado."
		Tip3 = "<b>Cargar nave (pegar sobre la existente)</b>: Esto agregará los planos de la nave guardados a la nave existente en la ubicación que elijas."
		Title = "Cargar planos"
	}
	LoadPaint
	{

		Tip = "Puedes cargarle el diseño de la pintura/calcomanía de otra nave guardada a tu nave actual. En el Editor de pintura, abre el menú <img name='hamburger_up'/> y luego haz clic sobre <white>Cargar diseño de pintura</white>."
		Title = "Cargar pintura"
	}
	LoadShapeFromPNG
	{

		Tip1 = "Puedes usar programas de arte externos como MS Paint, Paint.NET, Gimp o Photoshop para crear los contornos básicos de las formas de las naves..."
		Tip2 = "1. Primero, usa cualquier programa de arte externo que te guste para guardar un archivo de imagen PNG <b>blanco sobre negro</b> que represente la forma general de la nave. Los píxeles blancos se interpretarán como casillas de pasillos y los píxeles negros se interpretarán como espacios vacíos."
		Tip3 = "2. Luego, en el Editor de naves, abre el menú <img name='hamburger_up'/> y selecciona <white>Cargar forma desde PNG</white>. Selecciona el archivo de imagen PNG que guardaste."
		Tip4 = "3. Los planos de la nave actual serán reemplazados con los planos creados con el archivo de imagen PNG. Finaliza la nave agregando armas, propulsores, defensas y otras piezas que creas conveniente."
		Title = "Cargar forma desde PNG"
	}
	MidpointFocus
	{

		Tip = "Luego de atacar una nave enemiga, puedes hacer clic sobre el ícono del \"ojo\" para enfocar la vista en el punto medio entre tu nave y el objetivo."
		Title = "Ícono del punto medio de enfoque"
	}
	MirrorModeSettings
	{

		Tip = "Es probable que ya sepas que puedes activar el \"modo espejo\" para realizar cambios simétricos al diseño de la nave. También puedes <btn id='General.UIAltClick'/> sobre el botón del modo espejo para cambiar la dirección y cantidad de líneas simétricas."
		Title = "Ajustes del modo espejo"
	}
	MissionsHUD
	{

		Tip1 = "Por defecto, solo se muestran las 3 misiones más cercanas en la parte superior izquierda de la pantalla. Puedes cambiar esa cantidad en <good>Ajustes -> Experiencia de juego -> Superposiciones informativas -> Mostrar las misiones cercanas en la HUD</good>."
		Tip2 = "También puedes marcar una misión como \"favorita\" <white>haciendo clic derecho</white> sobre la misma o presionando el botón \"Favorita\" en la bitácora de carrera. Esto hará que la misión se muestre siempre, incluso si hay otras misiones más cerca."
		Title = "HUD de misiones"
	}
	ModAutoDisable
	{

		Tip = "Al actualizar Cosmoteer, todos los mods que hayas instalado <b>podrían</b> desactivarse de forma automática si no se sabe si son compatibles con la nueva versión de Cosmoteer. Si no quieres que los mods se desactiven de forma automática, entonces desactiva <good>Ajustes -> Varios -> Desactivar mods al actualizar</good>."
		Title = "Autodesactivar mods"
	}
	MovingResources
	{

		Tip = "Al usar la pantalla de Gestión de recursos para mover recursos, puedes mantener <btn id='Resources.Move1'/> para recoger o soltar 1 recurso a la vez en vez de toda la pila. Mantén <btn id='Resources.Move5'/> para que sean 5 a la vez."
		Title = "Mover recursos"
	}
	ObjectiveFocus
	{

		Tip = "Puedes hacer clic sobre la mayoría de los objetivos de las misiones para enfocar la vista sobre la ubicación a la que tendrás que volar para cumplir con ese objetivo."
		Title = "Enfocar objetivos"
	}
	PaintSchemes
	{

		Tip1 = "Los \"esquemas de pintura\" son conjuntos guardados de colores y texturas de pinturas que puedes compartir entre las naves."
		Tip2 = "Para guardar los colores y la textura de la nave actual, abre el menú <img name='hamburger_up'/> en el Editor de pintura y luego selecciona <white>Guardar esquema de pintura</white>. Para cargar ese esquema de pintura en una nave diferente, abre su menú <img name='hamburger_up'/> y selecciona <white>Cargar esquema de pintura</white>."
		Title = "Esquemas de pintura"
	}
	PartBrushes
	{

		Tip = "La mayoría de las piezas de tamaño 1x1 se pueden utilizar con una variedad de diferentes tamaños y formas de \"pinceles\". Haz clic en el botón de selección de pincel cuando esté disponible para ver todas las opciones."
		Title = "Pinceles de piezas"
	}
	PartRows
	{

		Tip = "Puedes aumentar la cantidad de tipos de piezas visibles en el Editor de naves cambiando <good>Ajustes -> Editor de naves -> Filas de piezas</good>."
		Title = "Filas del editor de naves"
	}
	PipSettings
	{

		Tip = "Puedes personalizar cuándo y qué muestra la vista de \"Imagen en imagen\" que aparece en la parte superior derecha de la pantalla en <good>Ajustes -> Cámara -> Imagen en imagen</good>."
		Title = "Ajustes de imagen en imagen"
	}
	PostShaders
	{

		Tip1 = "¿Quieres que Cosmoteer parezca una película de ciencia ficción en blanco y negro de los años 50? Si es así, entonces elige \"Vinieron del espacio\" en <good>Ajustes -> Gráficos -> Preferencias -> Postsombreado</good>."
		Tip2 = "¡También puedes probar varios estilos diferentes de divertidos \"postsombreados\"!"
		Title = Postsombreados
	}
	QuickSave
	{

		Tip1 = "Presiona <btn id='Game.QuickSave'/> para usar el guardado rápido sobre la partida actual."
		Tip2 = "Presiona <btn id='Game.QuickLoad'/> para cargar la última partida guardada de cualquier tipo."
		Tip3 = "Por defecto, Cosmoteer almacena los últimos 10 archivos de guardado rápido. Los archivos de guardado rápido más viejos se borrarán de forma automática. Puedes cambiar la cantidad de archivos de guardado rápido que se almacenan en <good>Ajustes -> Guardado automático</good>."
		Title = "Guardado rápido"
	}
	RebindingControls
	{

		Tip = "Casi todos los controles y teclas de acceso rápido se pueden cambiar en <good>Ajustes -> Controles</good>."
		Title = "Reasignar controles"
	}
	RecommendationBars
	{

		Tip = "Las barras que muestran la <b>tripulación</b> y la <b>energía</b> en la parte derecha del Editor de naves son solo <i>recomendaciones</i> para los nuevos jugadores. Las naves diseñadas de forma óptima podrían no requerir tanta tripulación o energía. Puedes desactivar estas barras en <good>Ajustes -> Editor de naves -> Mostrar barras de recomendaciones</good>."
		Title = "Barras de recomendaciones"
	}
	ReleaseCandidates
	{

		Tip1 = "Puedes ayudar a probar las actualizaciones preliminares de Cosmoteer si cambias a la versión beta \"inestable\" en las propiedades de Steam de Cosmoteer."
		Tip2 = "<bad>ADVERTENCIA:</bad> Estos \"aspirantes de lanzamiento\" podrían tener más errores que las actualizaciones oficiales. ¡Guarda la partida a menudo!"
		Title = "Aspirantes de lanzamiento"
	}
	ResourceCollectionTypes
	{

